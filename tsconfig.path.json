{
	"compilerOptions": {
		"paths": {
			"@/components/*": [
				"src/components/*"
			],
			"@/pages/*": [
				"src/pages/*"
			],
			"@/store/*": [
				"src/store/*"
			],
			"@/api": [
				"src/api"
			],
			"@/utils/*": [
				"src/utils/*"
			],
			"@/images/*": [
				"/public/images/*"
			],
			"@/dto/*": [
				"dto/*"
			],
			"@/context/*": [
				"src/context/*"
			],
			"@/data/*": [
				"src/data/*"
			],
			"@/types": [
				"src/types/"
			],
			"@/types/*": [
				"src/types/*"
			]
		}
	}
}